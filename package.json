{
  "name": "xfriend-fe-ts-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "postinstall": "husky install",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prepare": "husky install",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "lint": "eslint --fix --quiet src --ext .js,.svelte",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "npm run lint"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@mdi/js": "^7.2.96",
    "@smui-extra/accordion": "^7.0.0-beta.14",
    "@smui-extra/badge": "^7.0.0-beta.14",
    "@smui/button": "^7.0.0-beta.14",
    "@smui/data-table": "^7.0.0-beta.14",
    "@smui/dialog": "^7.0.0-beta.14",
    "@smui/fab": "^7.0.0-beta.14",
    "@smui/form-field": "^7.0.0-beta.14",
    "@smui/layout-grid": "^7.0.0-beta.14",
    "@smui/linear-progress": "^7.0.0-beta.14",
    "@smui/paper": "^7.0.0-beta.14",
    "@smui/slider": "^7.0.0-beta.14",
    "@smui/switch": "^7.0.0-beta.14",
    "@smui/tab": "^7.0.0-beta.14",
    "@smui/textfield": "^7.0.0-beta.14",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte": "^2.32.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "smui-theme": "^7.0.0-beta.14",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "dotenv": "^16.3.1",
    "graphql": "^16.7.1",
    "zod": "^3.21.4"
  }
}